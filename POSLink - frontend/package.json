{
  "name": "poslink-frontend",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "prestart": "npm install",
    "start": "ng serve --port 4200 --open",
    "build": "ng build --prod",
    "offline": "ng build --prod && http-server ./dist -p 9000 -o",
    "clean": "rimraf dist/ node_modules/ package-lock.json",
    "clean2": "rimraf node_modules/ package-lock.json",
    "rm:plants": "rimraf ./src/app/modules/plants",
    "define:modules": "npm run rm:plants && npm run define:module && npm run define:path",
    "define:module": "ng generate module plants --spec false --routing",
    "define:component": "ng generate component plants --spec false --module=app",
    "define:path": "cd ./src/app/modules/plants",
    "define:service": "ng generate service validation --spec false --module=app",
    "define:pipe": "cd ./src/app/plants && ng generate pipe timeAgo --spec false --module=plants"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.1.0",
    "@angular/common": "^6.1.0",
    "@angular/compiler": "^6.1.0",
    "@angular/core": "^6.1.0",
    "@angular/forms": "^6.1.0",
    "@angular/http": "^6.1.0",
    "@angular/platform-browser": "^6.1.0",
    "@angular/platform-browser-dynamic": "^6.1.0",
    "@angular/router": "^6.1.0",
    "@ngx-translate/core": "^10.0.2",
    "@ngx-translate/http-loader": "^3.0.1",
    "@swimlane/ngx-datatable": "^13.1.0",
    "ajv": "^6.5.2",
    "bootstrap": "^4.1.3",
    "core-js": "^2.5.7",
    "hoek": "^5.0.3",
    "jquery": "^3.3.1",
    "mydatepicker": "^2.6.4",
    "ngx-typeahead": "^6.0.2",
    "nodemailer": "^4.0.1",
    "popper": "^1.0.1",
    "popper.js": "^1.14.3",
    "rxjs": "^6.2.2",
    "rxjs-compat": "^6.2.2",
    "socks": "^2.1.6",
    "tunnel-agent": "^0.6.0",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.7.2",
    "@angular/cli": "^6.1.2",
    "@angular/compiler-cli": "^6.1.0",
    "@angular/language-service": "^6.1.0",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~10.5.5",
    "codelyzer": "^4.4.2",
    "jasmine-core": "~3.1.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~2.0.5",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^1.2.0",
    "protractor": "^5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "2.7.2"
  }
}
